	$(function(){

		//获取用户的登陆状态或者登陆后的开通状态
        function getUserState() {
            $.ajax({
                type : 'post',
                url : '../ajax/ktuProf_h.jsp?cmd=checkOpening',
                error : function(){
                },
                success : function(data){
                    var data = $.parseJSON(data);
                    changePageTips(data.state);
                }
            });
        }

        //如果登陆开通文案和链接改为如下state==2，如果登陆未开通链接改为如下state==1.state==0未登录不需要改变
        function changePageTips(state){
            
            var path = header.getManageUrl();
            var originalPath = "//"+portalHost + "/?f_ktu=true";

            if(state == 2) {
            	$('.toHomeBtn span').text('进入管理');
            	$('.toHomeBtn').attr('data-text','进入管理');
            	$('.toHomeBtn').attr('href',path);
            } else if(state == 1) {
                $('.toHomeBtn span').text('免费体验');
                $('.toHomeBtn').attr('data-text','免费体验');
				$('.toHomeBtn').attr('href',path);
            } else {
                $('.toHomeBtn span').text('免费体验');
                $('.toHomeBtn').attr('data-text','免费体验');
                $('.toHomeBtn').attr('href',originalPath);
            }
        }

        getUserState();


