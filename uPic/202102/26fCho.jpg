<script type='text/javascript'>

    var hasSession = false;
    var token = $('#_TOKEN') ? $('#_TOKEN').attr('value') : "";
    $(function(){
        var ktuManageDomain = "i.kt.fkw.com";
        var portalDomain = "i.fkw.com";
        /* Act on the event */
        document.addEventListener("visibilitychange",function(e){
            if(document.visibilityState=='visible'){
                $.ajax({
                    type:"post",
                    url:"/ajax/login_h.jsp?cmd=checkLoginState",
                    error:function(){

                    },
                    success:function(result){
                        var result = jQuery.parseJSON(result);
                        if (result.success) {
                            if(!hasSession || token != result.token){
                                window.location.reload();
                            }
                        }else {
                            if(hasSession){
                                window.location.reload();
                            }
                        }
                    }
                });
            }
        });
        $(".nav_btnArea .personal_manage .logout_btn").click(function(){
            _logout();
        });
        $("#login_btn").click(function(){
            //logDog(2000232, 10);
            popupLoginComponents.showPopupLoginWindow();
            popupLoginComponents.bindWeChatLogin();
        });
        function _logout(){
            if(confirm("确定要退出系统吗？")){
                $.ajax({
                    type: 'post',
                    url: '/ajax/login_h.jsp?cmd=logout',
                    error: function(error){
                        alert("服务繁忙，请稍后重试。");
                    },
                    success: function(result){
                        var result = jQuery.parseJSON(result);
                        if (result.success) {
                            // 不跳转了
                            window.location.reload();
                        } else {
                            alert(result.msg);
                        }
                    }
                });
            }
        }

    });
</script>



		

